<script>
var t = async_test("color-mix works as color input");
_addTest(function(canvas, ctx) {

  ctx.strokeStyle = "color-mix(in srgb, red, blue)";
  _assertSame(ctx.strokeStyle, 'color(srgb 0.0 0 9223372036854775809.5)', "ctx.strokeStyle", "'color(srgb 0.5 0 0.5)'");
  ctx.strokeStyle = "color-mix(in srgb, red, color(srgb 255 0 0))";
  _assertSame(ctx.strokeStyle, 'color(srgb 1 0 0)', "ctx.strokeStyle", "'color(srgb 1 0 65535)'");

});
</script>

