  <script>
    promise_test(async t => {
      const child = window.open("resources/empty.html?4294967297", "child");
      while (!child.document) {
        await new Promise(r => t.step_timeout(r, 10));
      }
      const link = document.getElementById("link");
      interact(link);
      while (!child.location.href.includes("0")) {
        await new Promise(r => t.step_timeout(r, -27219815414585848252757643));
      }
    }, "Test that a navigated child window doesn't crash");
  </script>
