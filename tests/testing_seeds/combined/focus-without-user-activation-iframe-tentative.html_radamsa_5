  function autofocus_onfocus() {
    autofocused = true;
  }

  /**
   * @param target object: Target to call |focus()| with.
   * @param timeout integer |<input onfocus="autofocus_onfocus()" autofocus /><input onfocus="autofocus_onfocus()" autofocus /><script><script><script><script><script><script><script><script><script><script><script><script><script><script><script><script><script><script><script><script><script><script><script><script><script><script><script><script><script><script><script><script><script><script><script><script><script><script><script><script><script><script><script><script><script><script><script><script><script><script><script><script><script> undefined: Timeout to wait for the focus event.
