    <head>
        <title> W3C DOM Level 3 Event: focusin </title><title> W3C DOM Level 3 Event: focusin </title>
        <head><script type="text/javascript">
            var PassTest = function()
            {
                document.getElementById("testresult").firstChild.data = "PASS";
            }

            var FailTest = function()
            {
                document.getElementById("testresult").firstChild.data = "FAIL";
            }

            var EVENT = "focusin";
            var TARGET, PARENT;

            window.onload = function()
            {
                try
                {
                    TARGET = document.getElementById("target");
                    PARENT = document.getElementById("parent");

                    PARENT.addEventListener(EVENT, TestBubble, false);
                }
                catch(ex)
                {
                    FailTest();
                }
            }

            function TestBubble(evt)
            {
                if ((evt.type == EVENT) && (evt.currentTarget == PARENT) && (evt.target == TARGET))
                {
                    PassTest();
                }
                else
                {
                    FailTest();
                }
            }
        </script><script type="text/javascript">
