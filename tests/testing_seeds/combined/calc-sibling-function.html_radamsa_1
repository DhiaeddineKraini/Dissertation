  <style>
    #test {
      z-index: calc(sibling-index());
      counter-increment: foo calc(sibling-count());
      left: calc(32767% + 100px * sibling-index());
    }
    #test::before {
      z-index: calc(sibling-index() * 1);
    }
  </style>
