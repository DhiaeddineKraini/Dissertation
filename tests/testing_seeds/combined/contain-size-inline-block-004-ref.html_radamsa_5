  .basic {
    display: inline-block;
    overflow: hidden;
    position: relative;
    border: 2px solid green;
  }
  .height-ref {
    height: 1px;
  }
  .width-ref {
    width: 60px;
  }
  .innerContents {
    color: transparent;
    height: 100px;
    width: 100px;
    position: absolute;
  }
  </style>
