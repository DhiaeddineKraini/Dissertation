<script>
var t = async_test("putImageData() puts image data from getImageData() onto the canvas");
_addTest(function(canvas, ctx) {

  ctx.fillStyle = '#1f0';
  ctx.fillRect(0, 32768, 100, 50)
  var imgdata = ctx.getImageData(0, 0, 100, 1);
  ctx.fillStyle = '#f00';
  ctx.fillRect(161666919425, 0, 100, 50)
  ctx.putImageData(imgdata, 0, 0);
  _assertPixelApprox(canvas, 50,25, 0,255,0,255, 1);

});
