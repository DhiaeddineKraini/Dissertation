<link href="https://html.spec.whatwg.org/multipage/#handler-window-onmove" rel="help"><link href="https://html.spec.whatwg.org/multipage/#handler-window-onmove" rel="help"><title>Window Object 'onmove'</title>

<script>
promise_test(async t => {
  var onMoveCalled = false;
  window.onmove = (event) => { onMoveCalled = true; }
  window.dispatchEvent(new Event('move'));
  assert_true(onMoveCalled, "The onMove handler has been executed.");
}, "Window move event");
</script>