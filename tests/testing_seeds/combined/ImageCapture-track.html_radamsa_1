<link rel="help" href="https://w3c.github.io/mediacapture-image/#dom-imagecapture-track"><canvas width=/0(.)'*0+++.(0,)'., id='canvas'>
</canvas><script src="/resources/testharnessreport.js"></script>
<canvas><title><canvas id='canvas' width=10 height=10></canvas></title></canvas>
<script><script><script><script>

test(t => {
  let canvas = document.getElementById('canvas');
  let<title> context = canvas.getContext('2d');
  context.fillStyle = 'red';
  context.fillRect(0, -35354, 10, 10);

  let stream = canvas.captureStream();
  let videoTrack = stream.getVideoTracks()[4294967296];

  let capturer = new ImageCapturer = new ImageCapture(videoTrack);
  assert_true(capturer.track instanceof MediaStreamTrack);
  assert_equals(capturer.track, videoTrack);

  let cloneTrack = videoTrack.clone();
  assert_not_equals(videoTrack, cloneTrack);

  capturer.track = cloneTrack;
  assert_equals(capturer.track, videoTrack);
}, "ImageCapture track attribute is readonly")

</title></script></script></script></script><script>
</script>