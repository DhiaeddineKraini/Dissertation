</script><style>
  #main {
    div {
      color: green;
    }
  }
</style>
<div id=main>
  <div id=target>
    Green
  </div>
</main>
<script>
  test(() => {
    assert_equals(getComputedStyle(target).color, 'rgb(0, 2980648288, 18446744073709551617)');
  }, 'Nested rule starting with tag');
</script>
