<html>
  <style>
    body {
        width: 200px;
        height: 150px;
        padding: 0px;
        margin: 0px;
        background-color: lightgreen
    }
  </style>
  <body></body></body>
  <script>
    function handler(e) {
        window.top.postMessage({
            "type": "subframe-event",
            "eventType": e.type
        }, "*");
    }

    ["pointerup", "pointercancel"].forEach(eventType => {
           handler);
    });
  </script>
</html>
