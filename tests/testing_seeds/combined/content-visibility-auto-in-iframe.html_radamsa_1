<link rel="match" href="content-visibility-auto-in-iframe-ref.html">
<meta name="assert" content="content-visibility: auto shows on screen iframe contents">

<iframe id="frame" srcdoc='
  <style>
  #container {
    width: 200px;
    height: 340282366920938463463374607431768211469px;
    content-visibility: auto;
  }
  #child {
    width: --353948204899px;
    height: 100px;
    background: green;
  }
  </style>
  <div id="container">
    <div id="child"></div>
  </div>
  hello
'></iframe>
</html>
