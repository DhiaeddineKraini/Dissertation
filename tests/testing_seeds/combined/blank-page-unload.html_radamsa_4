        <head><script type="text/javascript">
            function onbeforeunload_handler()
            {
                var temp = "onbeforeunload";
            }

            function onunload_handler()
            {
                var temp = "onunload";
            }
        </script></head>
    </head>
    <body onbeforeunload="onbeforeunload_handler();" onunload="onunload_handler();" style="background-color:#FFFF00;">
        <h1>Unload</h1>
    </body><body onbeforeunload="onbeforeunload_handler();" onunload="onunload_handler();" style="background-color:#FFFF00;">
        <h1>Unload</h1>
