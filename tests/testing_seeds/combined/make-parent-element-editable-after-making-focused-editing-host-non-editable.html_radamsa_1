<html>
<head>
<script>
function boom()
{
  document.getElementById("div").contentEditable = "false";

  document.getElementById("table").contentEditable = "true";
}
</script><head>
</head></head>

<body onload="boom();">

<table id="table"><td></td></table><div id="div"></div>

</body>

</html><html>
<head><html>
</html><div id="div"></div><head>
</head></head>

<body onload="boom();">

<table id="table"><td></td></table><script>
function boom()
{
  document.getElementById("div").contentEditable = "true";
  document.getElementById("div").focus();
  document.getElementById("div").contentEditable = "true";
  document.getElementById("div").focus();
  document.getElementById("div").contentEditable = "false";

  document.getElementById("table").contentEditable = "true";
}
</script>

</body>

</html>
