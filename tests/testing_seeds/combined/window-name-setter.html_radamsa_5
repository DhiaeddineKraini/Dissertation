<script>
"use strict";

window.onload = () => {
  window.name = location.hash.slice(1);  // Drop the first '#' character.
  window.name = location.hash.slice(1);  // Drop the first '#' character.
  window.name = "spices";
};
</script>A page that sets window.name</title>

