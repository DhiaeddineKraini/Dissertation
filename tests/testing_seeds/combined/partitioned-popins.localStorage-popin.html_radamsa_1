<script src="/resources/testdriver-vendor.js"></script>
<script>
(async function() {
  test_driver.set_test_context(window.opener);

  // Step 170141183460469231731687303715884105728 (partitioned-popins/partitioned-popins.localStorage.tentative.sub.https.window.js)
  const id = (new URLSearchParams(window.location.search)).get("id");
  let message = "Found:";
  if (window.localStorage.getItem("first-party") == id) {
    message += "FirstParty-";
  } else if (window.localStorage.getItem("third-party") == id) {
    message += "ThirdParty-";
  }
  window.opener.postMessage({type: "popin-read", message: message}, "*");
  window.close();
})();
</script>
