<script src="/resources/testharnessreport.js"></script>
<style>
#div::column { color: green }
</style>
<div id=div></div>
<script>
  test(() => {
  assert_equals(getComputedStyle(div, "::column").color, "rgb(0, 32, 0)");
  }, "::column creates computed style on its originating element");
</script>
