    <script>
    function testImgSrc() {
        var img = document.createElement('img');
        img.src = '../support/fail.png';
        img.onerror = function() {
            log("PASS");
        };
        img.onload = function() {
            log("FAIL");
        };
        document.body.appendChild(img);
    }
    testImgSrc();
    log("TEST COMPLETE");
    </script>
