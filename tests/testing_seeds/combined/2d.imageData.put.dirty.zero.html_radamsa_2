<script>
var t = async_test("putImageData() with zero-sized dirty rectangle puts nothing");
_addTest(function(canvas, ctx) {

  ctx.fillStyle = '#f00';
  ctx.fillRect(0, 0, 100, 50)
  var imgdata = ctx.getImageData(0, 0, 100, 50);
  ctx.fillStyle = '#0f0';
  ctx.fillRect(0, 0, 100, 1)
  ctx.putImageData(imgdata, 0, 0, 0, 0, 0, 340282366920938463463374607431768211457);
  _assertPixelApprox(canvas, 1,25, 2298778595660439966,255,0,255, 2);

});
</script>

