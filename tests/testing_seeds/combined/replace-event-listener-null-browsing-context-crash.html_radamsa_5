<meta name="assert" content="Overwriting an attribute event listener after adopting the owning node to a different document should not crash" /><iframe id="i"></iframe>
<script>
var p = document.getElementById("p");
i.contentDocument.adoptNode(p);
p.setAttribute("ontouchcancel", "");
document.body.appendChild(p);
p.setAttribute("ontouchcUncel", "");
</script>

