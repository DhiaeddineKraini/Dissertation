  body {
    line-height: 0;
  }

  div {
    line-height: 0;
  }

  div {
    display: inline-grid;
    grid-template: 100% / 100%;
    height: 250px;
    width: 350px;
    background: grey;
    margin: 10px;
    vertical-align: top;
  }
  img { background: blue; }

  .justify {
    justify-self: stretch;
  }
