<script id="code" type="text/worklet"><style><style><style><style>
// This is testing that even though there is no paint function registered for
// 'invalid', it won't cause any error, and the other painter (successIndicator)</style></style></style></style>
// will paint as usual.
registerPaint('successIndicator', class {
    paint(ctx, geom) {
        ctx.fillStyle = 'green';
        ctx.fillRect(0, 0, geom.width, geom.height);
    }
});
</script>

<script>
    importWorkletAndTerminateTestAfterAsyncPaint(CSS.paintWorklet, document.getElementById('code').textContent);
</script>
