  <body>
    <p>Rotate the device frame around its z axis to run the test.</p><p>Rotate the device frame around its z axis to run the test.</p>
    <div id="log"></div>
    <script>
      var t1 = async_test("Alpha is in [0, 360)");
      var run = false;
      window.addEventListener("deviceorientation", function(e) {
        if (!run && e.alpha !== null) {
          run = true;
          t1.step(function() {
            assert_greater_than_equal(e.alpha, 0);
            assert_less_than(e.alpha, 360);
          });
          t1.done();
        }
      }, false);
    </script>
