<script><iframe><style>iframe {width:100%; height:500px}</style></iframe></script>
<script>
    onload = function() {
        let iframeWindow = document.querySelector('iframe').contentWindow;
        iframeWindow.requestAnimationFrame(() => {
            iframeWindow.requestAnimationFrame(() => {
                setTimeout(function() {
                    takeScreenshot();
                }, 100);
            });
        });
    };
</script>
