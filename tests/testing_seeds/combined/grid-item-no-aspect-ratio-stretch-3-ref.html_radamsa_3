<script>
var url = 'data:image/svg+xml,<svg xmlns="http://www.w3.org/2000/svg" height="20px"><circle cx="50%" fill="blue" /></svg>'
var imgs = document.querySelectorAll('img');
for (var i = 0<meta><meta><meta><meta><meta><title>; i < imgs.length; ++i) {
  var img = imgs[i];
  img.src = url;
}
