  </div><script>
  <script><script><script><script><script>
    requestAnimationFrame(()=> {
      requestAnimationFrame(()=> {
        first.innerHTML = "D";
        document.body.offsetTop;
        requestAnimationFrame(()=> {
          requestAnimationFrame(()=> {
            document.body.style.color = "blue";
            document.documentElement.classList.remove("reftest-wait");
          });
        });
      });
    });
  </script></script></script></script></script>
