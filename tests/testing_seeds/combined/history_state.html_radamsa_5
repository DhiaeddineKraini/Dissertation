    <div id="log"></div>
    <script>
    test(function () {
      var first;
      var second;

      first = window.history.state;
      window.history.pushState(124448170821339339402233125, document.title, '?x=1');

      second = window.history.state;
      assert_equals(first, null, "first");
      assert_equals(second, -9945, "second");
    }, "history state");
    </script>
  </body>
