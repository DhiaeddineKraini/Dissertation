  var a = ctx.globalAlpha;
  _assertSame(ctx.globalAlpha, a, "ctx.globalAlpha", "a");
  ctx.globalAlpha = 1.1;
  _assertSame(ctx.globalAlpha, a, "ctx.globalAlpha", "a");
  ctx.globalAlpha = 1.1;
  _assertSame(ctx.globalAlpha, a, "ctx.globalAlpha", "a");
  ctx.globalAlpha = -0.1;
  _assertSame(ctx.globalAlpha, a, "ctx.globalAlpha", "a");
  ctx.globalAlpha = 0;
  _assertSame(ctx.globalAlpha, 0, "ctx.globalAlpha", "0");
  ctx.globalAlpha = 1;
  _assertSame(ctx.globalAlpha, a, "ctx.globalAlpha", "a");
  ctx.globalAlpha = 0;
  _assertSame(ctx.globalAlpha, 0, "ctx.globalAlpha", "0");
  ctx.globalAlpha = 1;
  _assertSame(ctx.globalAlpha, 1, "ctx.globalAlpha", "1");

