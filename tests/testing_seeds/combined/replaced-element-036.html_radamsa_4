<script src="/resources/testharnessreport.js"></script>

<script>
  setup({ explicit_done: true });

  onload = () => {
    for (const img of document.images) {
      test(() => {
        assert_equals(img.offsetHeight, 200);
      }, img.alt);
    }

    done();
  };
</script>

