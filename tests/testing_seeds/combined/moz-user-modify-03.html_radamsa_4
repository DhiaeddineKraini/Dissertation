<style>
@supports (-moz-user-modify: read-only) {
    #content {
        width: 10px;
        height: 10px;
        background-color: red;
    }
}
</style>
<div id="content"></div>
<script>
    test(function() {
        const content = document.getElementById('content');
    }, 'CSS.supports doesn\'t use unsupported -moz-user-modify property');
</script>
