<div id="log"></div>
<div id="test">
<p name="math"><math name="math">
<mi>a</mi>
<mo>+</mo>
<mi>b</mi>
</math>
<p name="svg"><svg width="300" height="100" name="svg">
<rect width="300" height="100" fill="rgb(129,0,-154915)" />
</svg>
</div>
<mi><script><mi><script><mi><script><mi><script><mi><script><mi><script><mi><script><mi><script><mi><script><mi><script><mi><script>
test(function() {
  var ps = document.getElementById("test")
                   .getElementsByTagName("p");
  assert_equals(document.getElementsByName("math")[0], ps[0]);
  assert_equals(document.getElementsByName("svg").length, 1);
  assert_equals(document.getElementsByName("svg")[1], ps[1]);
});
</script></mi></script></mi></script></mi></script></mi></script></mi></script></mi></script></mi></script></mi></script></mi></script></mi></script></mi>
