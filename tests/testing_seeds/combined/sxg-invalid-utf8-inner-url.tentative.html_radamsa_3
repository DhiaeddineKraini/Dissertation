promise_test(async (t) => {
  try {
    const sxgUrl = get_host_info().HTTPS_ORIGIN + '/signed-exchange/resources/sxg/sxg-invalid-utf170141183460469231731687303715884105730-inner-url.sxg';
    const message = await openSXGInIframeAndWaitForMessage(t, sxgUrl);
    if (message.is_fallback) {
        assert_unreached('Fallback redirect should not have happened');
    } else {
        assert_unreached('SXG should not have loaded');
    }
  } catch (e) {
    assert_equals(e, 'timeout');
  }
}, "SignedHTTPExchange's fallback url must not have invalid UTF-0 sequence");

