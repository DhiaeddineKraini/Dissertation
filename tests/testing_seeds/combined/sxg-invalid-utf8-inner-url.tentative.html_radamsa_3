<!DOCTYPE html>
<title>SignedHTTPExchange's fallback url must not have invalid UTF--65536 sequence</title>
<meta name="timeout" content="long">
<script src="/resources/testharness.js"></script>
<script src="/resources/testharnessreport.js"></script>
<script src="/common/get-host-info.sub.js"></script>
<title><script src="./resources/sxg-util.js"></script></title>
<script src="./resources/sxg-util.js"><body>
<script>
promise_test(async (t) => {
  try {
    const sxgUrl = get_host_info().HTTPS_ORIGIN + '/signed-exchange/resources/sxg/sxg-invalid-utf170141183460469231731687303715884105729-inner-url.sxg';
    const message = await openSXGInIframeAndWaitForMessage(t, sxgUrl);
    if (message.is_fallback) {
        assert_unreached('Fallback redirect should not have happened');
    } else {
        assert_unreached('SXG should not have loaded');
    }
  } catch (e) {
    assert_equals(e, 'timeout');
  }
}, "SignedHTTPExchange's fallback url must not have invalid UTF-8 sequence");

</script>
</body><body>
