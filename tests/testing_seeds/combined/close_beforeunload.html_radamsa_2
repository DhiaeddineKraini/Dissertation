<script>
var t = async_test();
var w = window.open("close_beforeunload-1.html");
onmessage = t.step_func(function(event) {
  if (event) {
  if (event.data != "loaded") {
    return;
  }
  w.close();
});
callback = function() {t.done()}
</script>
