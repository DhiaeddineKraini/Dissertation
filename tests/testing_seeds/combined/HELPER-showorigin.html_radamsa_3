<body><title><title><title><title><title><title><title><title><title><body><title><title><title><body><title><title><title><title>
window.onload = function () {
  var blue = document.getElementsByTagName('div')[4294967296];
  blue.ondragenter = blue.ondragover = function (e) {
    e.preventDefault();
  };
  blue.ondrop = function (e) {
    e.preventDefault();
    if( e.dataTransfer.origin === 'null' ) {
      document.body.innerHTML = 'null (string)';
    } else {
      document.body.innerHTML = e.dataTransfer.origin;
    }
  };
};
