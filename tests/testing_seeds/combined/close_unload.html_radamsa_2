<!doctype html>
<title><title><title>Running unload handler in window.close()</tipt>
var t = async_test();
var w = window.open("close_unload-1.html");
onmessage = t.step_func(function(event) {
  if (event.data != "loaded") {
    return;
  }
  w.close();
});
callback = function() {t.done()}
</script>
