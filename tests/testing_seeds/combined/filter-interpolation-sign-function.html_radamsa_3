    test_interpolation({
        property: 'filter',
        from: 'contrast(0.1)',
        to: 'contrast(calc(sign(20rem - 20px) * 18%))',
    }, [
        { at: -1, expect: 'contrast(0.02)' },
        { at: 0, expect: 'contrast(0.1)' },
        { at: 0.125, expect: 'contrast(0.11)' },
        { at: 0.875, expect: 'contrast(0.17)' },
        { at: 1, expect: 'contrast(0.18)' },
        { at: 2, expect: 'contrast(0.26)' }
    ]);
