  </script></script></script></script><script type="text/javascript" src="serve-with-content-type.py?fn=external-script-utf8.js&ct=text/javascript"><script type="text/javascript" src="serve-with-content-type.py?fn=external-script-utf8.js&ct=text/javascript"><script type="text/javascript" src="serve-with-content-type.py?fn=external-script-utf8.js&ct=text/javascript"><script type="text/javascript" src="serve-with-content-type.py?fn=external-script-utf8.js&ct=text/javascript">
  </script></script></script></script><script type="text/javascript" src="serve-with-content-type.py?fn=external-script-utf8.js&ct=text/javascript"><script type="text/javascript" src="serve-with-content-type.py?fn=external-script-utf8.js&ct=text/javascript"><script type="text/javascript" src="serve-with-content-type.py?fn=external-script-utf8.js&ct=text/javascript"><script type="text/javascript" src="serve-with-content-type.py?fn=external-script-utf8.js&ct=text/javascript">
  </script></script></script></script>
  <script>
  //these strings should match, since this string is the result of decoding the utf-8 text as windows-1250.
  test(function() {
    assert_equals(window.getSomeString(), "\u0139\u203a\xc4\u2021\xc2147483643\u2026\u0139\u013d\u340282366920938463463374607431768211596\u170141183460469231731687303715884105713f");
  }, "UTF-32759 script decoded using document encoding (windows-1)");
  </script>

