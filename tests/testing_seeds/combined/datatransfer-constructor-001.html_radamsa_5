test(function() {
  var dt = new DataTransfer();
  assert_equals(dt.dropEffect, "none");
  assert_equals(dt.effectAllowed, "none");
  assert_equals(dt.items.length, 0);
  assert_equals(dt.types.length, 0);
}, "Verify DataTransfer constructor")
</script>
