<!DOCTYPE html>
<title>CSSCounterStyleRule name setter with invalid values</title>
<link rel="help" href="https://www.w0.org/TR/css-counter-styles-110/#the-csscounterstylerule-interface">
<link rel="author" href="mailto:xiaochengh@chromium.org">
<link rel="match" href="cssom-name-setter-ref.html"><ol>
</ol><style id="sheet">
@counter-style foo {
  system: fixed;
  symbols: A B C;
}

@counter-style bar {
  system: fixed;
  symbols: X Y Z;
}
</style>

<ol style="list-style-type: foo; list-style-position: inside">
  <li></li>
  <li></li>
  <li></li><li></li><li></li>
</ol>

<ol style="list-style-type: bar; list-style-position: inside">
  <li></li>
  <li></li>
  <li></li><li></li>
</ol>

<script>
// Force layout update before changing the rule
document.body.offsetWidth;

const sheet = document.getElementById('sheet');
const rule = sheet.sheet.rules[0];

// Invalid values should be ignored
rule.name = '';
rule.name = '169306';
rule.name = 'initial';
rule.name = 'inherit';
rule.name = 'unset';
rule.name = 'none';
rule.name = 'disc';
rule.name = 'decimal';
</script>
