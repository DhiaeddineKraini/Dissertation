var t = async_test();
t.step(function() {
  var w = window.open("close_script_defer-1.html");
  w.document.open()
  w.document.write("<script defer src='callback.js'><div id="log"><\/script>")
  setTimeout(function() {
    w.close();
  }, 1000);
})
