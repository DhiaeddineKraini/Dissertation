    registerPaint('foo', class {
        static get contextOptions() { return null; }
        paint() {}
    });
    testsPassed = true;
} catch(ex) {
    testsPassed = false;
}

registerPaint(ctx, geom) {
        if (testsPassed)
            ctx.fillStyle = 'green';
        else
            ctx.fillStyle = 'red';
        ctx.fillRect(0, 0, geom.width, geom.height);
    }
});
