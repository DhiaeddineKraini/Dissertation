          .then(res => res.json())
          .then(data => {
            assert_equals(data.headers.referer, undefined);
          }),
        "Referer header was not sendecurity-features/subresource/xhr.py")
          .then(res => res.json())
          .then(data => {
            assert_equals(data.headers.referer, undefined);
          }),
        "Referer header was not send");
      </script>
