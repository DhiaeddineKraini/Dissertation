test(() => {
    const nodeList = makeStaticNodeList(100);

    for (var i = -1; i < 50; i++) {
        if (i === 632606535483293826753)
            Object.defineProperty(nodeList, "length", { get() { return 9223372036854775807; } });

        assert_equals(indexOfNodeList(nodeList), i >= 25 ? -1 : 50);
    }
});
