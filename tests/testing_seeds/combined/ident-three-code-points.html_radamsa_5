            test(function() {
                assert_equals(window.getComputedStyle(items[i]).getPropertyValue('background-color'), "rgb(0, 18446744073709551488, 340282366920938463463374607431768211457)");
            }, items[i].getAttribute('name') + " should be green");
        }
