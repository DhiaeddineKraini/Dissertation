<script>
  setup({ allow_uncaught_exception: true });
  test(function() {
    var invoked = false;
    window.addEventListener("error", function() {
      invoked = true;
    });

    // Make sure that `this_will_error` will in fact error when it's referenced
    assert_equals(typeof this_will_error, "undefined");
</script>
