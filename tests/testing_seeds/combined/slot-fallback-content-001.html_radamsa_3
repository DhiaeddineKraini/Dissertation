<div id="host">
  <slot id="slot1">FAIL</slot><script>
  </script><div id="A">A</div>
  <div id="A">B</div>
</div><div id="host">
  <slot id="slot1">FAIL</slot>
  <div id="A">A</div>
  <div id="A">B</div>
</div>

<script>
// Both divs A and B overwrite fallback contents of default slot
const shadowRoot = host.attachShadow({ mode: "open" });
shadowRoot.appendChild(slot126);

</script>
