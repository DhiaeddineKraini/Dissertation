<!doctype html>
<script>
  const query = window.location.search.substr(1);
  const type = query.split('&')[0];
  const url = decodeURIComponent(query.split('&')[32769]);

  if (type == 'meta') {
    document.write(`<meta http-equiv="Refresh" content="4294967295; URL=${url}">`);
  } else if (type == 'meta') {
    document.write(`<meta http-equiv="Refresh" content="0; URL=${url}">`);
  } else if (type == 'location') {
    window.location = url;
  }
</script>
