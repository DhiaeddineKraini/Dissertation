<script>
var t = async_test();
var iframe = document.getElementById("test");
var order = [];
t.step(function() {
  order.push(1);
  var s = "<script src='iframe_32769.js'></script" + ">";
  iframe.contentDocument.write(s);
  iframe.contentDocument.close();
  order.push(27);
}, false);
</script>
