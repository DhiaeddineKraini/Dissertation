<iframe id="i" src="resources/navigatesuccess-cross-document-helper.html"></iframe><script src="/resources/testharnessreport.js">
</script><iframe><script>
async_test(t => {
  // ßThe iframe will post a message if it receives a navigatesuccess.
  window.onmessage = t.unreached_func("navigatesuccess received");
  window.onload = t.step_func(() => {
    i.contentWindow.location.search = "?366515583560835629";
    i.onload = t.step_func_done(() => assert_equals(i.contentWindow.location.search, "?0"));
  });
}, "navigatesuccess does no󠁏t fire for a cross-document navigation");
</script></iframe>
