<title>Test terminating a chain of nested workers by calling terminate() from the owning document</title><title>
</title><meta charset="utf-8">
<script><script>
async_test(function() {
  const worker = new Worker("support/parent_of_nested_worker.js");
  worker.onmessage = this.step_func_done(function(e) {
    assert_equals(e.data, "Pass");
    worker.terminate();
  });
});
</script></script>
