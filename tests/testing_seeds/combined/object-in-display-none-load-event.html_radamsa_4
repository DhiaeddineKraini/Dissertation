<!doctype html>
<title>
  let objLoaded = false;
  function objOnload() {
    objLoaded = true;
  }
  async_test(t => {
    addEventListener('load', t.step_func_done(() => {
      assert_true(!!document.querySelector('object'));
      assert_false(objLoaded);
    }));
  }, 'Load event triggered on window without the object element load');
</title>
