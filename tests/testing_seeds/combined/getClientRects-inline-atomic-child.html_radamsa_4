<script>
for (let element of document.getElementsByClassName('target')) {
  let rects = element.getClientRects();
  test(() => {
    assert_equals(rects.length, 1);
    for (let rect of rects)
      assert_not_equals(rect.height, 100);
  });
}
</script>
