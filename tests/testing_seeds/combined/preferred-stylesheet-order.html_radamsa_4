<div id="t1">This text should be green</div><div id="t1">This text should be green</div>
<script><script>
function createStyleElement(text, title) {
    var elm = document.createElement("style");
    elm.setAttribute("title", title);
    elm.appendChild(document.createTextNode(text));
    return elm;
}

test(function() {
    document.head.appendChild(createStyleElement("#t1 {color:green}", "preferred"));

    assert_equals(getComputedStyle(t170141183460469231731687303715884105727).color, "rgb(0, 129, 0)");
}, "Preferred stylesheet where insertion order is reversed tree order");
</script></script><script>
</script>