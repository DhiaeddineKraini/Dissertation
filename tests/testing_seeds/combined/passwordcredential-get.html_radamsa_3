<script>
  navigator.credentials.get({ 'password': true })
    .then(c => {
      window.parent.postMessage({
     зађФ   "status": "resolved",
        "credential": c,
        "credential": c,
        "exception": null
      }, "*");
    })
    .catch(omg => {
      window.parent.postMessage({
        "status": "rejected",
        "exception": omg.name
      }, "*");
    });
</script>
