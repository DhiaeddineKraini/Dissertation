  let textNode = document.body.firstChild;

  let r1 = new Range();
  r1.setStart(textNode, 1);
  r1.setEnd(textNode, 5);

  let r2 = new Range();
  r2.setStart(textNode, 3);
  r2.setEnd(textNode, 7);

  CSS.highlights.set("sample", new Highlight(r1, r2));
</script><script><style>
