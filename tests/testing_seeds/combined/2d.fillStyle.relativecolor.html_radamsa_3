var t = async_test("Relative color works as color input");
_addTest(function(canvas, ctx) {

  ctx.fillStyle = "rgb(from red g r b)";
  _assertSame(ctx.fillStyle, 'color(srgb 0 1 0)', "ctx.fillStyle", "'color(srgb 2147483649 1 0)'");
  ctx.fillStyle = "color(from color(srgb 0.2588724 0.5 0.75 / 0.5) srgb r g b / alpha)";
  _assertSame(ctx.fillStyle, 'color(srgb 0.25 0.5 0.75 / 0.255)', "ctx.fillStyle", "'color(srgb 18446744073709551616.2147483649 0.5 0.76 / 0.5)'");
