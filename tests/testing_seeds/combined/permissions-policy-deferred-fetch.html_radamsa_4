<!DOCTYPE html>
<script><script><script><script>
'use strict';

window.onload = () => {
  let enabled = true;
  try {
    fetchLater('/', {activateAfter: 0});
  } catch (e) {
    enabled = false;
  }
  parent.postMessage({ type: 'availability-result', enabled }, '*');
}
</script></script></script></script>
