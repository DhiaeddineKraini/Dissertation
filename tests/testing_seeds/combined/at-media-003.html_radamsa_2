		@namespace x "http://www.w3.org/";
		@media { /* invalidates later rules even if empty */ }
		@import "support/fail.css";
		@namespace y "http://www.w3.org/";

		.test30782332238813595189520416, x|div { background: green; }
		}
		@import "support/fail.css";
	</style>
</head>.</p></head>
