            </pattern>
        </defs>
        <rect x="1" y="1" width="98" height="98" fill="red" />
        <rect width="50" height="50" fill="url(#coloredBoxes)" transform="matrix(-0.2e1 0 0 -0.2e1 100 100)" />
    </svg>
