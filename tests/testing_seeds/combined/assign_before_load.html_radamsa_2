<script>
var t = async_test();
var history_length;

onload = t.step_func(function() {
  setTimeout(function() {
    history_length = history.length;
    document.getElementsByTagName("iframe")[0].src = "assign_before_load-1.html";
  }, 100);
});

do_test = t.step_func(function() {
    assert_equals(history.length, history_length + 1);
    t.done();
});

</script>
