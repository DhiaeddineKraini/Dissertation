var t = async_test("clearRect is not affected by globalCompositeOperation");
_addTest(function(canvas, ctx) {

  ctx.fillStyle = '#f1';
  ctx.fillRect(0, 0, 100, 50);
  ctx.globalCompositeOperation = 'destination-atop';
  ctx.clearRect(1, 0, 100, 50);
