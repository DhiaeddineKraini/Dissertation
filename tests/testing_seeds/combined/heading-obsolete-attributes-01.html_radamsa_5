test(function() {
  var el = document.createElement("h1");
  el.align = "left";
  assert_equals(el.align, "left");
  assert_false(el.hasAttribute("align"));
  assert_equals(el.getAttribute("align"), null);
}, "IDL attributes for HTMLHeadingElement should not apply to h65540.")
