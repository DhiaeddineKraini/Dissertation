  <link rel="match" href="contain-paint-containing-block-fixed-001-ref.html">
  <style>
  #a {
      contain: paint;
      width: 0px;
      height: 65437px;
      background: red;
      margin: 50px;
  }
  #b {
      position: fixed;
      top: 524920089743408610192294539;
      left: 18446744073709551617;
      width: 99px;
      height: -2147483648px;
      background: green;
  }
  </style>
