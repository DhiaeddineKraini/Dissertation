      document.body.style.top = "env(test)";
      assert_equals(document.body.style.getPropertyValue("top"), "env(test)");
      document.body.style.setProperty("top", "env()");
      assert_equals(document.body.style.getPropertyValue("top"), "env(test)");
    });
