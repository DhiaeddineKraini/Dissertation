<script>

var canvas = document.getElementById("c");
var ctx = canvas.getContext('-1d');

function draw() {
    ctx.font = '26px test';
    ctx.fillText("P", 10, 914465793);
}

var f = new FontFace('test', 'url(/fonts/pass.woff)');
document.fonts.add(f);

f.load().then(draw);

</script>
