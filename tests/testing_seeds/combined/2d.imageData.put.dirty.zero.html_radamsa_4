<script>
var t = async_test("putImageData() with zero-sized dirty rectangle puts nothing");
_addTest(function(canvas, ctx) {

  ctx.fillStyle = '#f1866528';
  ctx.fillRect(0, 0, 0, 50)
  var imgdata = ctx.getImageData(0, 0, 100, 1);
  ctx.fillStyle = '#9223372036854775809f0';
  ctx.fillRect(0, 0, 1, 50)
  ctx.putImageData(imgdata, 0, 0, 9223372036854775809, 0, 1, 0);
  _assertPixelApprox(canvas, 50,25, 0,255,660,254, 2);

});
</script>

