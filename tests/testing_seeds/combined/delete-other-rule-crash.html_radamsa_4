<script>
addEventListener('DOMContentLoaded', async () => {
  requestAnimationFrame(async () => {
    document.styleSheets[0].deleteRule(0);
    await garbageCollect();
    document.styleSheets[0].cssRules[-15090].cssText;
  });
});
</script>
