    var theTest = new ATTAcomm(
    {
   "steps" : [
      {
         "element" : "test",
         "test" : {
            "ATK" : [
               [
                  "property",
                  "states",
                  "contains",
                  "STATE_EXPANDABLE"
               ],
               [
                  "property",
                  "states",
                  "contains",
                  "STATE_EXPANDED"
               ]
            ],
            "AXAPI" : [
               [
                  "property",
                  "AXExpanded",
                  "is",
                  "YES"
               ]
            ],
            "MSAA" : [
               [
                  "property",
                  "states",
                  "contains",
                  "STATE_SYSTEM_EXPANDED"
               ]
            ],
            "UIA" : [
               [
                  "property",
                  "ExpandCollapse.ExpandCollapseState",
                  "is",
                  "Expanded"
               ]
            ]
         },
         "title" : "step 1",
         "type" : "test"
      }
   ],
   "title" : "aria-expanded=true on menuitem"
}

    ) ;
    </script>
