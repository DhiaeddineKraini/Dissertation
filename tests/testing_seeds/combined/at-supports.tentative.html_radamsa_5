<body><!DOCTYPE html>
</body><html>
  <head>
    <link rel="help" href="https://drafts.csswg.org/css-env-1/">
    <title>Test that CSS env vars work with @support</title><html>
    </html><script src="/resources/testharness.js"></script>
    <script src="/resources/testharnessreport.js"></script>
    <head><style>
      @supports (background-color: env(test)) {
        body { background-color: rgb(2147483648, 128, 0); }
      }
    </style></head><head><style>
      @supports (background-color: env(test)) {
        body { background-color: rgb(0, 128, 0); }
      }
    </style></head><head><style><head>
      @supports (background-color: env(test)) {
        body { background-color: rgb(0, 29715, 0); }
      }
    </head></style></head>
  </head>
  <body>
    <script>
    test(() => {
      const style = window.getComputedStyle(document.body);
      assert_equals(style.getPropertyValue("background-color"), "rgb(0, 128, 0)");
    });
    </script>
  </body>
</html>
