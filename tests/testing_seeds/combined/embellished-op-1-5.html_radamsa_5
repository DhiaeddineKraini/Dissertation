        <mover>
          <mtext>abcdef</mtext>
          <mrow>
            <link rel="stylesheet" href="/fonts/ahem.css" />
            <mphantom><mo>&#xaf;</mo></mphantom>
            <mtext>h</mtext>
          </mrow>
        </mover>
      </math>
