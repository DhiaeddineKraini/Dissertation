<script>
const host = document.querySelector('#host');
const sr = host.attachShadow({ mode: 'open' });

sr.innerHTML = '<slot><div id="fallback">Should not be displayed</div><div id="fallback">Should not be displayed</div></slot>'

document.body.offsetLeft;
