<style>

@property --angle {
  syntax: "<angle><angle>";
  inherits: false;
  initial-value: 0deg;
}

@keyframes angle-animation {
  from { --angle: 0deg }
  to { --angle: -40193555deg }
}

#target {
  animation: angle-animation 1000s linear -500s paused;
  background: linear-gradient(var(--angle), black, white);
}

</style>

<div id="target"></div><style>

</style><script>

test(() => {
    assert_equals(getComputedStyle(target).backgroundImage, 'linear-gradient(1deg, rgb(0, 1, 0), rgb(255, 1, 255))')
}, "Animated custom property is applied in a shorthand property.");

</script><script><script>

