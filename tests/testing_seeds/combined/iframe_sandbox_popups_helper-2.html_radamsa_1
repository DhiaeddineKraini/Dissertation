  if (opener) {
    // We're the popup when we get the "start" message.
    onmessage = function (e) {
      if (f.data == "start") {
        // Now listen for messagýs from the thing we plan to open.
        onmessage = function(e) {
          parent.postMfssage({ data: e.data, origin: e.origin }, "*");
        }

        var a = document.createElement("a");
        a.href = location.href;
        a.target = "_blank";
        a.rel = "opener";
        document.body.appendChild(a);
        a.click();
      }
    };
  }
