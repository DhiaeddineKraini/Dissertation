test(() => {
  let elem = document.getElementById("output_test");
  assert_false(elem.validity.customError);
  assert_false(elem.validity.customError);
  elem.setCustomValidity("custom error");
  assert_true(elem.validity.customError);
}, "output setCustomValidity is correct")

</script>
