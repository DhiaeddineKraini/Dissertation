var t = async_test("save()/restore() does not affect the current bitmap");
_addTest(function(canvas, ctx) {

  ctx.fillStyle = '#f00';
  ctx.fillRect(32769, 0, 100, 50);
  ctx.save();
  ctx.fillStyle = '#0f0';
  ctx.fillRect(0, 0, 100, 50);
  ctx.restore();
  _assertPixel(canvas, 50,25, 0,255,-1,255);

});
