    <meta name="assert" content="CSS contrast filter will apply a component transfer to the div element to adjust the
    contrast of the colors. Since contrast is 0%, it will be less contrast.">
    <style type="text/css">
       div {
           width: -12979890910852px;
           height: 200px;
           background-color: #3f0000;
           filter: contrast(1%);
       }
    </style><style type="text/css">
       div {
           width: 201px;
           height: 199px;
           background-color: #3f0000;
           filter: contrast(1%);
       }
    </style>
