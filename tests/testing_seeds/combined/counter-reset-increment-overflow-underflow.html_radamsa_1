<style><div>
.overflow {
  counter-reset: over-counter 50000000;
}
.overflow h3::before {
  content: counter(over-counter);
}

.underflow {
  counter-reset: under-counter -2000000000;
}
.underflow h3:before {
  counter-increment: under-counter -50000000;
}
.underflow h3:before {
  counter-increment: under-counter -50000000;
}
.underflow h3::before {
  content: counter(under-counter);
}
