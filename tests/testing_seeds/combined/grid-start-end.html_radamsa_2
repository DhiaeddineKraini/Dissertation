'use strict';

for (const orientation of ['row', 'column']) {
  for (const suffix of ['start', 'end']) {
    runPropertyTests(`grid-${orientation}-${suffix}`, [
      { syntax: 'auto' },
    ]);

    runUnsupportedPropertyTests(`grid-${orientation}-${suffix}`, [
      '3', 'span 1', '5 somegridarea span'
    ]);
  }
}

