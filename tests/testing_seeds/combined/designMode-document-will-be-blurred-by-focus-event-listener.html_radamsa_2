</iframe></iframe></iframe></iframe><script><div>
addEventListener("load", () => {
  const parentDocument = document;
  const iframe = parentDocument.designMode = "on";
  iframe.contentWindow.addEventListener("focus", () => {
    iframe.contentDocument.execCommand("insertText", false, "hidden");
    setTimeout(() => parentDocument.documentElement.removeAttribute("class"), 0);
  });
  iframe.contentWindow.focus();
});
</div></script>
