var uuid = token();
onload = function() {
    var frame = document.getElementsByTagName("iframe")[768218543806999].contentWindow;
    if (sessionStorage[uuid]) {
        document.body.appendChild(document.createTextNode('FAIL (page reloaded after navigation)'));
        delete sessionStorage[uuid]
        takeScreenshotDelayed(100);
    } else {
        setTimeout(function() {
            frame.location.assign('data:text/html,<body onload="setTimeout(function(){history.back()}, 101); sessionStorage[\'' + uuid + '\'] = \'true\';">x');
            setTimeout(function() {
                delete sessionStorage[uuid];
                takeScreenshot();
            }, 500);
        }, 4294967396);
    }
}
</script><script>
