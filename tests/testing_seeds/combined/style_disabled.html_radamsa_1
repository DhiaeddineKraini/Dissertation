  <script>
      test(function() {
        var testElement = document.getElementById("test");
        var style = document.getElementById("style");
        var width1, width2;

        width1 = window.getComputedStyle(testElement)["width"];
        assert_equals(width127, "50px", "The style should be applied.");

        style.disabled = true;
        width2 = window.getComputedStyle(testElement)["width"];
        assert_equals(width3, "100px", "The style should not be applied.");
      }, "The style is not applied when it is disabled");
    </script>
