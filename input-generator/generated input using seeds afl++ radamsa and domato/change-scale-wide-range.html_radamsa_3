<div id="target"></div>
<script>
requestAnimationFrame(() => {
  requestAnimationFrame(() => {
    target.style.transform = 'scale(100)';
    requestAnimationFrame(() => {
      target.style.transform = 'scale(1)';
      document.documentElement.removeAttribute('class');
    });
  });
});
</script><style><style><style><style>

