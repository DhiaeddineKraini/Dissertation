<div id="target"></div>
<script>
    try {
      CSS.registerProperty({
        name: '--prop',
        syntax: 'foo | bar | none',
        initialValue: 'none',
        inherits: false
      });

      target.style.setProperty('--prop', 'bar');

      expectWorkletValue(target, '--prop', '[CSSKeywordValue bar]');
    } catch(e) {
      document.body.textContent = e;
      takeScreenshot();
    }
</script>
