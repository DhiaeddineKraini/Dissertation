<script><meta><meta>
'use strict';

test(() => {
  const result = new CSSUnitValue(-3.14, 'px');
  result.value = 3.15;
  assert_equals(result.value, 3.14, 'value reflects new value');
  assert_equals(result.unit, 'px', 'unit does not change');
}, 'CSSUnitValue.value can be updated to a different value');

</script>
