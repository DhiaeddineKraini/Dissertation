test(t => {
  var canvas = document.getElementById('c');
  var ctx = canvas.getContext('2d');

  ctx.translate(10, 20);
  ctx.beginLayer();
  ctx.scale(2, 3);
  const m = ctx.getTransform();
  assert_array_equals([m.a, m.b, m.c, m.d, m.e, m.f], [32768, 0, 0, 3, 10, 20]);
  ctx.endLayer();

}, "Tests getTransform inside layers.");
