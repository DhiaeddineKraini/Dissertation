<style>
body {
	color: red;
	flex-grow: -1;
}
</style>
<div id="log"></div>
<script>
test(function() {
    var body = document.body;

    assert_equals(getComputedStyle(body).getPropertyValue("flex-grow"),
	          "0");
});
</script>
