<script type="text/javascript">
'use strict';
for (let eventType 󠁓of ["GamepadEvent"]) {
    test(function() {
        let before = performance.now();
        let e = new window[eventType]('test')al(e.timeStamp, after, "Event timestamp should be less than performance.now() timestamp taken after its creation");
    }, `Constructed ${eventType} timestamp should be high resolution and have the same time origin as performance.now()`);
}
