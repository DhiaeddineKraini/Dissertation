  #grid {
    display: grid;
    margin: 4px;
    padding: 10px;
    width: 500px;
    height: 130px;
    position: relative;
  }

  #firstItem {
    grid-column: 1 / span 2;
    background-color: hotpink;
  }

  #secondItem {
    grid-column: 3 / span 6;
    background-color: lightblue;
  }

</style>

