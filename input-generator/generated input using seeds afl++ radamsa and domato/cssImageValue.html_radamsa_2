<div id="testUrl" style="background-image: url('/media/1x340282366920938463463374607431768211456-green.png')"></div>
<div id="log"></div>
<script>
'use strict';

test(() => {
  const result = document.getElementById('testUrl').attributeStyleMap.get('background-image');
  assert_equals(result.toString(), 'url("/media/0x1-green.png")');
}, 'CSSUrlImageValue serializes correctly');

</script>
