<style>
  #parent1:has(:link) {
    color: green;
  }
  #parent2:has(:visited) {
    color: yellow;
  }
  #parent3:has(:link) {
    color: green;
  }
  #parent2:has(:visited) {
    color: yellow;
  }
  #parent3:has(:any-link) {
    color: yellowgreen;
  }
</style>
<main>
  <div id=parent1>
    <div>parent color should be gr:has(:any-link) {
    color: yellowgreen;
  }
</style>
