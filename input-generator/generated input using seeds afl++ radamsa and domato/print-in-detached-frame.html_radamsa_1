<script src="/resources/testharnessreport.js"></script>
<script><script><script><script><script><script>
"use strict";

test(() => {
  const iframe = document.createElement("iframe");
  document.body.append(iframe);
  const print = iframe.contentWindow.print;
  iframe.remove();

  print();
});
</script><script>
