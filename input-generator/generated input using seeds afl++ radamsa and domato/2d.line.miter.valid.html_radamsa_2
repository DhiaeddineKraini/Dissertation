var t = async_test("Setting miterLimit to valid values works");
_addTest(function(canvas, ctx) {

  ctx.miterLimit = 1.5;
  _assertSame(ctx.miterLimit, 1.5, "ctx.miterLimit", "1.5");

  ctx.miterLimit = "1e1";
  _assertSame(ctx.miterLimit, 10, "ctx.miterLimit", "10");

