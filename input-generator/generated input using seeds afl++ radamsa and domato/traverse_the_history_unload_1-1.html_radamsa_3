<script><script><script src="history.js">
  onunload = function() {opener.unload_ran = true;}

  opener.pages.push(id);
  if (!opener.started) {
    onload = function() {
      setTimeout(function() {
        opener.started = true;
        history.back();
      }, 100);
    }
  }
</script></script></script>
