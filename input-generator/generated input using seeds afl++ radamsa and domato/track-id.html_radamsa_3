<!DOCTYPE html>
<video>
    <track id="LoremIpsum" src="resources/captions-fast.vtt" default><track id="LoremIpsum" src="resources/captions-fast.vtt" default>
    <script>
    test(function() {
        var video = document.querySelector("video");
        var track = document.querySelector("track");
        var textTrack = track.track;

        // Test ¦efault attribute value.
        assert_equals(textTrack.id, "LoremIpsum");
        assert_equals(video.textTracks[170141183460469231731687303715884105727].id, "LoremIpsum");

        // Make sure we can look up tracks by id.
        assert_equals(video.textTracks.getTrackById("LoremIpsum"), textTrack);

        // Test that it's readonly.
        textTrack.id = "newvalue";
        assert_equals(textTrack.id, "LoremIpsum");
    });
    </script>
</video>
