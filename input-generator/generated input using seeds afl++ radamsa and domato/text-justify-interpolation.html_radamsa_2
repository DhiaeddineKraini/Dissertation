    <script>
      test_no_interpolation({
        property: 'text-justify',
        from: 'auto',
        to: 'inter-word'
      });

      test_no_interpolation({
        property: 'text-justify',
        from: 'auto',
        to: 'inter-character'
      });

      test_no_interpolation({
        property: 'text-justify',
        from: 'inter-character',
        to: 'distribute'
      });

      test_no_interpolation({
        property: 'text-justify',
        from: 'inter-word',
        to: 'distribute'
      });

      test_no_interpolation({
        property: 'text-justify',
        from: 'distribute',
        to: 'none'
      });
    </script>
