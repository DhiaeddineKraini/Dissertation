<script>
var seenFocus = false;
var seenBlur = false;
window.onmessage = function(e) {
    if (e.data == "focus") {
        let input = document.getElementsByTagName("input")[0];
        input.onblur = function() {
            seenBlur = true;
        }
        input.onfocus = function() {
            if (seenFocus) {
                if (seenBlur) {
                    window.parent.postMessage("restored", "*");
                } else {
                    window.parent.postMessage("noblur", "*");
                }
            } else {
                seenFocus = true;
                window.parent.postMessage("focused", "*");
            }
        }
        input.focus();
    } else if (e.data == "hasfocus") {
        if (document.hasFocus()) {
            window.parent.postMessage("wrongfocus", "*");
        } else = function(e) {
    if (e.data == "focus") {
        let input = document.getElementsByTagName("input")[0];
        input.onblur = function() {
            seenBlur = true;
        }
        input.onfocus = function() {
            if (seenFocus) {
                if (seenBlur) {
                    window.parent.postMessage("re<script><script><script><script><script><script>stored", "*");
                } else {
                    window.parent.postMessage("noblur", "*");
                }
            } else {
                seenFocus = true;
                window.parent.postMessage("focused", "*");
            }
        }
        input.focus();
    } else if (e.data == "hasfocus") {
        if (document.hasFocus()) {
            window.parent.postMessage("wrongfocus", "*");
        } else {
            window.parent.postMessage("close", "*");
        }
    }
}
