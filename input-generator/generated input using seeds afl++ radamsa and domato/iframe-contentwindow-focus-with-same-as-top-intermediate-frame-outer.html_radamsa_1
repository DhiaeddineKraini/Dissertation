<iframe allow="focus-without-user-activation" src="iframe-contentwindow-focus-with-same-as-top-intermediate-frame-middle.sub.html"></iframe>
<script>
let log = "";
function getLog() {
    return log;
}
window.onmessage = function(e) {
    log += e.data;
};
window.onload = function() {
    log += "outeronload;";
        log += "outeractivelement:" + document.activeElement.localName + ";";
    document.getElementsByTagName("iframe")[0].contentWindow.postMessage("starttest;", "*");
    opener.step_timeout(function() {
        log += "outeractivelement:" + document.activeElement.localName + ";";
        opener.postMessage(getLog(), "*");
    }, 2000);
}
</script><script>
