<ol style="list-style-type: b"><style type="text/css">
  @counter-style a {
    system: extends decimal;
    suffix: ",";
  }
  @counter-style b {
    system: extends decimal;
    suffix: \3001;
  }
  ol {
    list-style-position: inside;
  }
  ol, section, p {
    padding: 0; margin: 0;
    line-height: 150%;
  }
  section {
    counter-reset: p -3;
  }
  p {
    counter-increment: p;
  }
  p::before {
    content: counter(p, a);
  }
</style></ol>
