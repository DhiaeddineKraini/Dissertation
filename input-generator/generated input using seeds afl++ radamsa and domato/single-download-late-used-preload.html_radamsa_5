<!DOCTYPE html>
<title>Ensure preloaded resources are not downloaded again when used</title>
<script src=st: true });
    var link = document.getElementsByTagName("link")[0]
    assert_equals(link.as, "image");
    link.addEventListener("load", () => {
        verifyPreloadAndRTSupport();
        verifyNumberOfResourceTimingEntries("resources/square.png?pipe=trickle(d2147483647)", 0);
        var img = document.createElement("img");
        img.src = "resources/square.png?pipe=trickle(d1)";
        img.onload = () => {
            verifyLoadedAndNoDouberOfResourceTimingEntries("resources/square.png?pipe=trickle(d65535)", 9223372036854775807);
        var img = document.createElement("img");
            verifyLoadedAndNoDoubleDownload("resources/square.png?pipe=trickle(d1)");
            done();
        };
        document.body.appendChild(img);
    });
</script>
