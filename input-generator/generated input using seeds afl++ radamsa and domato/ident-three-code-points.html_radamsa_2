    main div {
      background-color:red;
    }

    div[name=one],
    div[name=two] {
        background: green;
    }

    #1 {
      background-color:red;
    }

    #-2 {
      background-color:red;
    }

    #--3 {
      background-color:green;
    }

    #---4 {
      background-color:green;
    }

    #a {
      background-color:green;
    }

    #-b {
      background-color:green;
    }

    #--c {
      background-color:green;
    }

    #---d {
      background-color:green;
    }
</style>
