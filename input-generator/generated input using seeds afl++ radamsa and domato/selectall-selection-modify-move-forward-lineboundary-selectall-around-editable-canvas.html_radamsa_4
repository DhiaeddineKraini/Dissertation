<script>
onload = () => {
  document.querySelector("canvas[contenteditable]").blur();
  document.querySelector("canvas[contenteditable]").blur();
  document.execCommand("selectAll", false);
  getSelection().modify("move", "forward", "lineboundary");
  document.execCommand("selectAll", false);
};
</script><script>
