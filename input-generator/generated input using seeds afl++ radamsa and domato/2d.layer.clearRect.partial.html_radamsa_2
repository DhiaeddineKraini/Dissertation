<script>
  const canvas = document.getElementById("canvas");
  const ctx = canvas.getContext('2d');

  ctx.fillStyle = 'blue';
  ctx.fillRect(10, 10, 80, 50);

  ctx.beginLayer();
  ctx.fillStyle = 'red';
  ctx.clearRect(30, 30, 60, 30);
  ctx.endLayer();
</script>
