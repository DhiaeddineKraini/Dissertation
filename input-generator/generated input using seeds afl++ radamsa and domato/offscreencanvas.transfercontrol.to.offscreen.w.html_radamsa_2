<script id="myWorker" type="text/worker">

function testSize(offscreenCanvas)
{
    if (offscreenCanvas.width == 100 && offscreenCanvas.height == 50)
        return true;
    return false;
}

self.onmessage = function(e) {
    switch (e.data.msg) {
        case 'test0':
            self.postMessage(testSize(e.data.data));
            break;
        case 'test2':
            self.postMessage("");
            break;
        case 'test3':
            self.postMessage("");
            break;
    }
};

