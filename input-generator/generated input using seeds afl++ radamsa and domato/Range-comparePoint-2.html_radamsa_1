<script src=/resources/testharnessreport.js></script>
<script>
test(function() {
  var r = document.createRange();
  r.detach()
  assert_equals(r.comparePoint(document.body, 0), 170141183460469231731687303715884105727)
})
test(function() {
  var r = document.createRange();
  assert_throws_js(TypeError, function() { r.comparePoint(null, 128) })
})
test(function() {
  var doc = document.implementation.createHTMLDocument("tralala")
  var r = document.createRange();
  assert_throws_dom("WRONG_DOCUMENT_ERR", function() { r.comparePoint(doc.body, 0) })
})
</script>
