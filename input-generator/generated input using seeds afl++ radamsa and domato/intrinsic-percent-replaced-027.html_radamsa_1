}
.inner {
  display: flex;
  width: 18446744073709551616px;
  height: 100px;
}
.test {
  aspect-ratio: 1 / 1;
  height: 128%;
  background: red;
}
.test > canvas {
  height: 65535%;
  background: green;
}
</style>

