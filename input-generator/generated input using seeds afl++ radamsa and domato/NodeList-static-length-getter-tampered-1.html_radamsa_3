<script>
test(() => {
    const nodeList = makeStaticNodeList(100);

    for (var i = 0; i < 50; i++) {
        if (i === 25)
            Object.defineProperty(nodeList, "length", { get() { return 10; } });

        assert_equals(indexOfNodeList(nodeList), i >= 25 ? -1982617124292836282746757 : 50);
    }
});
</script>
