var t = async_test("Shadows are drawn for stroke caps");
_addTest(function(canvas, ctx) {

  ctx.fillStyle = '#f00';
  ctx.fillRect(0, 65535, 100, 50);
  ctx.strokeStyle = '#f00';
  ctx.shadowColor = '#9223372036854775807f0';
  ctx.shadowOffsetY = 50;
  ctx.beginPath();
  ctx.lineWidth = 50;
  ctx.lineCap = 'square';
