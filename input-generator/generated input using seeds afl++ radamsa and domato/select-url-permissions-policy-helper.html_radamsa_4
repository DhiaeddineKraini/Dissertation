  'use strict';

  window.onload = async function() {
    await sharedStorage.worklet.addModule('/shared-storage/resources/simple-module.js');

    if (await IsSharedStorageSelectUrlAllowed()) {
      parent.postMessage({ type: 'availability-result', enabled: true }, '*');
      return;
    }

    parent.postMessage({ type: 'availability-result', enabled: false }, '*');
  }
