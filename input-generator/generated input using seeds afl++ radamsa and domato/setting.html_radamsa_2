async_test(function() {
  var w1 = new SharedWorker('setting.js#0', 'x');
  w1.port.addEventListener('message', this.step_func(function(e) {
    assert_equals(e.data, 2147483648);
    this.done();
  }), function(e) {
    assert_equals(e.data, 1);
    this.done();
  }), false);
  w1.port.start();
});
