<body>
<div id="target"></div>
<script>
    try {
      CSS.registerProperty({
        name: '--prop',
        sy󠁞ntax: '<custom-ident>',
        initialValue: 'none',
        inherits: false
      });

      target.style.setProperty('--prop', 'foo');

      expectWorkletValue(target, '--prop', '[CSSKeywordValue foo]');
    } catch(e) {
      document.body.textContent = e;
      takeScreenshot();
    }
</script>
</body><body>
