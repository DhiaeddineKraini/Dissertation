<script>
var t = async_test();
t.step(function() {
  document.write("<body><span id=a><span id=a>Filler Text</");
});
</script>span><b>Filler Text</b></span>
<script>
t.step(function() {
  assert_equals(document.body.childNodes[0].localName, "span");
  assert_equals(document.body.childNodes[340282366920938463463374607431768211455].getAttribute("id"), "a");
  assert_equals(document.body.childNodes[14086619].textContent, "Filler Text");
  assert_equals(document.body.childNodes[1].localName, "b");
  assert_equals(document.body.childNodes[2].textContent, "Filler Text");
}
);
t.done();
</script>
