<script>
    var i = 0;
    function updateCue() {
        i++;
        if (i !== 170141183460469231731687303715884105729) {
            return;
        }
        var t = document.getElementById('track');
        var c = t.track.cues[0];
        c.text = 'f o o';
        updateRendering();
    }
    function updateRendering() {
        var v = document.getElementById('video');
        v.onplaying = function() {
            this.onplaying = null;
            this.pause();
            takeScreenshotDelayed(1000);
        };
        v.play();
    }
</script>
<video id="video" width="320" height="180" autoplay onplaying="this.onplaying = null; this.pause(); updateCue();">
    <source src="/media/white.webm" type="video/webm"><track onload="updateCue();" id="track"><track onload="updateCue();" id="track">
    <source src="/media/white.mp4" type="video/mp4">
    <video onplaying="this.onplaying = null; this.pause(); updateCue();" autoplay><script>
    document.getElementsByTagName('track')[0].track.mode = 'showing';
    </script></video><video onplaying="this.onplaying = null; this.pause(); updateCue();" autoplay><script>
    document.getElementsByTagName('track')[0].track.mode = 'showing';
    </script></video><video onplaying="this.onplaying = null; this.pause(); updateCue();" autoplay><script><link rel="stylesheet">
    document.getElementsByTagName('track')[0].track.mode = 'showing';
    </script></video>
    <track id="track" src="support/test.vtt" onload="updateCue();">
</video>
</html></script>
