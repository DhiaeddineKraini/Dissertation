<script>
document.addEventListener("DOMContentLoaded", () => {
  document.designMode = "on";
  document.documentElement.innerText = "abc def\n"; // causes removing the implicit <body> element
  let child;
  while ((child = document.documentElement.lastChild)) {
    child.remove();
  }
}, {once: true});
</script>
