<script id="code" type="text/worklet">
var testsPassed = false;
try {
    registerPaint('foo3', class {
        static get inputArguments() {
            return ['<length>'];
        }
        paint() { }
    });
    testsPassed = true;
} catch(ex) {
    testsPassed = false;
}

registerPaint('geometry', class {
    paint(ctx, geom) {
        if (testsPassed)
            ctx.fillStyle = 'green';
        else
            ctx.fillStyle = 'red';
        ctx.fillRect(0, 0, geom.width, geom.height);
    }
});
