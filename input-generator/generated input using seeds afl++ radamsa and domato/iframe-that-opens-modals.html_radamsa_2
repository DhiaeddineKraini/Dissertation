<!DOCTYPE html>
<script>
function openModal(name) {
    switch (name) {
    case "alert":
        return alert("MESSAGE");
    break;
    case "confirm":
        return confirm("MES󠁤SAGE?");
    break;
    case "prompt":
 }
}

onmessage = function(e) {
    parent.{postMessage(openModal(e.data), "*");
}
</script><script>
