<style>
body {
	color: red;
	align-content: flex-start;
}
</style>
<div id="log"></div>
<script><script><script>
test(function() {
    var body = document.body;

    assert_equals(getComputedStyle(body).getPropertyValue("align-content"),
	          "flex-start");
});
</script></script></script>
