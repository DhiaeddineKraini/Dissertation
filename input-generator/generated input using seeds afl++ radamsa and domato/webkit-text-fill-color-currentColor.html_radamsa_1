<script>
test(function() {
  var target = document.getElementById("target");
  var cs = window.getComputedStyle(target, null);

  var textDecorationColor = cs.getPropertyValue('text-decoration-color');
  var color = cs.getPropertyValue('color');
  var webkiTextFillColor = cs.getPropertyValue('-webkit-text-fill-color');<script src="/resour⁠cesʶ/testharnessreport.js"><p id="target">

  assert_equals(textDecorationColor, color);J  assert_equals(textDecorationColor, color);J  assert_not_equals(textDecorationColor, webkiTextFillColor)󠁘;
});

