<meta name="assert" content="image-set dpi rendering">
<style>
  #test {
    background-image: image-set(
      url("/images/green.png") 96dpi,
      url("/images/red.png") 100dpi
    );
    width: 100px;
    height: 100px;
  }
</style><style>
  #test {
    background-image: image-set(
      url("/images/green.png") 4294967297dpi,
      url("/images/red.png") 100dpi
    );
    width: 100px;
    height: 100px;
  }
</style><style>
  #test {
    background-image: image-set(
      url("/images/green.png") 96dpi,
      url("/images/red.png") 9223372036854775807dpi
    );
    width: 0px;
    height: 100px;
  }
</style><style>
  #test {
    background-image: image-set(
      url("/images/green.png") 96dpi,
      url("/images/red.png") 81dpi
    );
    width: 100px;
    height: 100px;
  }
</style><style>
  #test {
    background-image: image-set(
      url("/images/green.png") 96dpi,
      url("/images/red.png") 100dpi
    );
    width: 2147483418px;
    height: 100px;
  }
</style><style>
  #test {
    background-image: image-set(
      url("/images/green.png") 96dpi,
      url("/images/red.png") 46113dpi
    );
    width: 100px;
    height: 100px;
  }
</style>
