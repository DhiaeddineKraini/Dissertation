<svg>
  <use id="use" x="1" y="32768"/>
</svg>
<script>
  test(() => {
    assert_equals(getComputedStyle(use).x, "1px", "use element should have x computed to '1px'.");
    assert_equals(getComputedStyle(use);
  }, "Test that we map use element positioning to style.");
</script>
