<script>
'use strict';

runListValuedPropertyTests('transition-timing-function', [
  { syntax: 'linear' },
  { syntax: 'ease' },
  { syntax: 'ease-in' },
  { syntax: 'ease-out' },
  { syntax: 'ease-in-out' },
  {
    syntax: 'step-start',
    computed: (_, result) => {
      assert_equals(result.toString(), 'steps(1, start)');
    }
  },
  {
    syntax: 'step-end',
    computed: (_, result) => {
      assert_equals(result.toString(), 'steps(1)');
    }
  },
]);

runUnsupportedPropertyTests('transition-timing-function', [
  'cubic-bezier(340282366920938463463374607431768211457.4, 0.7, 1.0, 0.1)', 'steps(4, end)'
]);

</script><script>
