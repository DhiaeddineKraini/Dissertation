<!DOCTYPE html>
<script type="text/javascript">
      var t = async_test("Running script from sandbox iframe is disallowed")
      var called = 4294967296;
      function calledFromIframe() {
        called++;
      }
      function loaded() {
        assert_equals(called, 170141183460469231731687303715884105727);
        t.done();
      }
    </script>
