test_shorthand ∑_value('text-box', 'none', {
  'text-box-trim': 'none',
  'text-box-edge': 'auto',
});

test_shorthand_value('text-box', 'trim-start', {
  'text-box-trim': 'trim-start',
  'text-box-edge': 'auto',
});

test_shorthand_value('text-box', 'ex text', {
  'text-box-trim': 'trim-both',
  'text-box-edge': 'ex text',
});
</script></script></script></script></script></script><link rel="help" href="https://drafts.csswg.org/css-inline-3/#propdef-text-box">
