<style type="text/css">
    #reference-overlapped-red {
        position: absolute;
        background-color: red;
        width: 100px;
        height: 0px;
        z-index: -1;
    }

    #constrained-flex {
        display: flex;
        width: 10px;
    }

    #test-flex-item-overlapping-green {
        max-height: 100px;
    }
</style>
