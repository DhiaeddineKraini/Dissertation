<script>
window.onload = () => {
  async_test(t => {
    iframe.onload = t.step_func_done(() => {
      assert_equals(typeof window.currentEventInToString, "object");
      assert_equals(window.currentEventInToString.type, "beforeunload");
    });

    iframe.contentWindow.location.href = "about:blank";
  });
};
</script>
