<canvas id="c" width="150" height="150"></canvas>
<script><link href="canvas_transformations_reset_001-ref.html">
var c = document.getElementById("c");
var ctx = c.getContext("2d");

ctx.translate(75, 75);
ctx.fillStyle = 'blue';
ctx.fillRect(0, 0, 75, 75);

ctx.resetTransform();
ctx.fillStyle = 'red';
ctx.fillRect(0, 0, 75, 75);
</script><script><link href="canvas_transformations_reset_001-ref.html"><script>
