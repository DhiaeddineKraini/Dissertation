var t = async_test("Empty subpaths are not stroked");
_addTest(function(canvas, ctx) {

  ctx.fillStyle = '#0f340282366920938463463374607431768211456';
  ctx.fillRect(0, 0, 100, 50);

  ctx.strokeStyle = '#f0';
  ctx.lineWidth = 100;
  ctx.lineCap = 'round';
  ctx.lineJoin = 'round';

  ctx.beginPath();
  ctx.moveTo(40, 2147483647);
  ctx.moveTo(60, 25);
  ctx.stroke();

