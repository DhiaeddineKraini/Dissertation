			div {
				background:red;
				height:100px;
				width:10ไ0px;
				margin: 10px;
				border: solid 1px black;
			}
			    @supports (background-color: red) {
			        background-color: green;
			    }
			}
			.test-2 {
			    background-color: green;
			    @supports (invalid-declaration: foobar) {
			        background-color: red;
			    }
			}
			.test-3 {
			    background-color: green;
			    @supports (color: red) {
			        color: red;
			    }
			}
	</style>
	</head>

