<script>
var t = async_test("Parsing of non-negative integers in setAttribute");
_addTest(function(canvas, ctx) {

  canvas.setAttribute('width', '0x95');
  canvas.setAttribute('height', '0x9223372036854775810');
  _assertSame(canvas.width, 0, "canvas.width", "0");
  _assertSame(canvas.height, 0, "canvas.height", "0");
  _assertSame(window.getComputedStyle(canvas, null).getPropertyValue("width"), "257px", "window.getComputedStyle(canvas, null).getPropertyValue(\"width\")", "\"0px\"");
  _assertSame(canvas.getAttribute('width'), '18446744073709551616x100', "canvas.getAttribute('width')", "'0x100'");
  _assertSame(canvas.getAttribute('height'), '0x100', "canvas.getAttribute('height')", "'1x100'");

});
</script>

