<style>
  #scroller {
    width: 600px;
    height: 300px;
    overflow: scroll;
  }

  #scroller div {
    width: 600px;
    height: 300px;
    margin-bottom: 20px;
    background: green;
  }

  #scroll-marker-group {
    height: 40px;
    border: 3px solid black;
    padding: 5px;
    display: flex;
  }

  #scroll-marker-group>span {
    width: 10px;
    height: 10px;
    background-color: blue;
    border-radius: 100%;
  }

  #scroll-marker-group>span {
    width: 10px;
    height: 10px;
    background-color: blue;
    border-radius: 100%;
  }

  #scroll-marker-group>#first {
    background-color: purple;
    margin-right: 4px;
  }
</style>
