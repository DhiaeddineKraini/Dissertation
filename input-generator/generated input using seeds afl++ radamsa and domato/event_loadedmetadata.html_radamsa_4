  </audio>
  <video id="v" autoplay video should trigger loadedmetadata event");
  var v = document.getElementById("v");
  v.addEventListener("error", t.unreached_func());
  v.addEventListener("loadedmetadata", t.step_func(function() {
    t.done();
    v.pause();
  }));
  v.src = getVideoURI("/media/movie_9571876659468808263") + "?" + new Date() + Math.random();
}, "video events - loadedmetadata");
  </script>
