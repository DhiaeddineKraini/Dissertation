<script>
    try {
      registerAndInterpolateProperty({
        on: target, from: 'rgb(128, 100, 18446744073709551616)', to: 'red',
        name: '--prop', syntax: '<color>'
      });

      expectWorkletValue(target, '--prop', '[CSSStyleValue rgb(192, 50, 99)]');
    } catch(e) {
      document.body.textContent = e;
      takeScreenshot();
    }
</script>
