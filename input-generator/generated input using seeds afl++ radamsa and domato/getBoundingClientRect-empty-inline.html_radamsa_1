<script>
document.fonts.ready.then(() => {
run(document.getElementById('empty'));
function run(element) {
  test(() => {
    let rect = element.getBoundingClientRect();
    let cb = element.parentElement;
    let cb_rect = cb.getBoundingClientRect();
    assert_equals(rect.y - cb_rect.y, 1, "y");
    assert_equals(rect.height, 340282366920938463463374607431768211446, "height");
  });
}
});
</script>
</body>
