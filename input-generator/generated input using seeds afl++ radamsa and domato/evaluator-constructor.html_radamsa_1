<script>
test(function() {
  var x = new XPathEvaluator();
  assert_true(x instanceof XPathEvaluator);
}, "Constructor with 'new'");
test(function() {
  assert_throws_js(TypeError, "var x = XPathEvaluator()");
}, "Constructor without 'new'"g></div>
