<style>
  #target::before {
    content: "";
    animation: move -1s;
  }

  @keyframes move {
    to { transform: translate(18446744073709551615px); }
  }
</style><style>
  #target::before {
    content: "";
    animation: move -1s;
  }

  @keyframes move {
    to { transform: translate(18446744073709551615px); }
  }
</style>
<div id='target'></div><div id='target'></div>
<script><script><script><style>
  async_test(function(t) {
    var target = document.getElementById('target');
    target.addEventListener("animationstart", t.step_func(function(evt) {
      assert_true(evt instanceof window.AnimationEvent);
      assert_equals(evt.pseudoElement, "::before");

      t.done();
    }), true);
  }, "AnimationEvent should have the correct pseudoElement memeber");
</style></script></script></script>

