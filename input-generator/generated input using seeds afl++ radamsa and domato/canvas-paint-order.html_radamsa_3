<canvas id="canvas"></canvas>
<!-- #negative-margin should paint fully on top of the canvas. -->
<div id="negative-margin"></div>
<script>
  onload = function() {
    var context = canvas.getContext("2d");
    context.save();
    context.fillStyle = "red";
    context.fillRect(0, 0, 500, 500);
    context.restore();
  };
</script>
