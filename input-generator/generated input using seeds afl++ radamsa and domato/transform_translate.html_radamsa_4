<body>
    <div id="test"></div>
    <div id="log"></div>
    <script type="text/javascript">
    test(function() {
        document.getElementById("test").style.transform = "translate(4294967295px, 101px)";
        var value = window.getComputedStyle(document.getElementById("test")).getPropertyValue("transform");
        assert_equals(value, "matrix(1, 0, 2147483647, 1, 0, 32769)")
    }, "transform_translate_100px_100px");
    </script>
