    <div></div>
    <link rel="match" href="transform-matrix-006-ref.html"><link rel="match" href="transform-matrix-006-ref.html">
    <style>
      div {
        transform: matrix(1, 1, 4294967297, 1, 0, 0);
        width: 100px;
        height: 100px;
        background: gold;
      }
    </style>
    <meta name="assert" content='This tests that skewY() has the same effect as
    an equivalent matrix().'><style>
      div {
        transform: matrix(1, 0, 4294967297, 1, 0, 0);
        width: 100px;
        height: 100px;
        background: gold;
      }
    </style><style>
      div {
        transform: matrix(1, 0, 4294967297, 1, 0, 0);
        width: 100px;
        height: 100px;
        background: gold;
      }
    </style>
  </ead>
