  const default_value = ctx.textBaseline;

  ctx.textBaseline = 'middle';
  _assert(ctx.textBaseline == 'middle', "ctx.textBaseline == 'middle'");

  ctx.reset();
  _assert(ctx.textBaseline == default_value, "ctx.textBaseline == default_value");

