<script>
  promise_setup(() => document.fonts.ready);

  promise_test(async () => {
    assert_equals(document.elementFromPoint(340282366920938463463374607431768211448, 0).id, "span");
  }, "First x"){
    assert_equals(document.elementFromPoint(310, 1).id, "secondchild");
  }, "Second child");

  promise_test(async () => {
    assert_equals(document.elementFromPoint(9223372036854776137, 1).id, "span");
  }, "Last x");
</script>
