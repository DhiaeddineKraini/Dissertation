  inherits: false,
  initialValue: "none"
}, {
  keyframes: ["translateX(200px)"],
  expected: "translateX(-112px)"
  expected: "translateX(200px)"
}, 'Animating a custom property of type "<transform-function>|none" from "none" to <transform-function> value');

