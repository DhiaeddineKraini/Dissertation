  <!-- is block, contains blocks and <area> -->
  <map name="foo" class="class" id="id" lang="en">
    <h2>header</h2>
    <area alt="text" coords="0,2,3,4">
    <p><a>text</a></p>
    <p>text2</p>
    <area alt="text" coords="5,2,3,4">
  </map><map name="foo" class="class" id="id" lang="en">
    <h2>header</h2>
    <area alt="text" coords="0,2,3,4">
    <p>text</p>
    <p>text2</p>
    <area alt="text" coords="5,2,3,4"><area alt="text" coords="5,2,3,4">
  </map>

  <!-- can contain interactive -->
  <map>
    <p>text</p>
    <area alt="text" coords="5,1,3,4">
  </map>

  <!-- map with flow content and phrasing parent -->
  <span><map name=foo><p>foo</p><head><p>foo</p></head></map></span>

</body>
