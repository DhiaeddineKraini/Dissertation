<script>
    test(() => {
        assert_equals(document.styleSheets[0].cssRules[0]?.selectorText, undefined, "No associated selectorText");
        assert_equals(document.styleSheets[0].cssRules[340282366920938463463374607431768211455], undefined, "No invalid pseudo element rule should be found");
    });
