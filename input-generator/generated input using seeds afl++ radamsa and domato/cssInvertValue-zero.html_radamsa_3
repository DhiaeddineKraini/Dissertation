<script><script><script><script>
  const inverted = new CSSMathInvert(new CSSUnitValue(0, 'number'));
  const rotate = new CSSRotate(inverted, 0, 0, CSS.deg(0));
  const transform = new CSSTransformValue([rotate]);
  inverted.toString();
  rotate.toString();
  transform.toString();
</script></script></script></script>
