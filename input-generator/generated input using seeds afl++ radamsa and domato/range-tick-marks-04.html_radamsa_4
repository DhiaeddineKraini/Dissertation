<option value="30" disabled>
<input type="range" min="-100" max="100" value="0" step="10" name="power" list="powers">
<datalist id="powers">
  <option value="0" disabled>
  <option value="-30">
  <link rel="match" href="range-tick-marks-04-ref.html">
  <option value="50">
</datalist>
