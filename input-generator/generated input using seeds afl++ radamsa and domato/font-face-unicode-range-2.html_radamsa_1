<style>
  @font-face {
    font-family: base;
    src: url(support/fonts/LigatureSymbolsWithSpaces.woff);
  }
  @font-face {
    font-family: swoosh;
    src: url(support/fonts/Rochester.otf);
    unicode-range: U+27;
  }
  .test {
    font-family: swoosh, base;
  }
  .ref {
    font-family: base;
  }
  .ref .amp {
	  font-family: swoosh, base;
  }
  div {
	  font-size: 5em;
  }
</style><style>
