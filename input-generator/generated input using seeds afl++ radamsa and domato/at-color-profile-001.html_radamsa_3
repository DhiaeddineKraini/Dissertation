<div class="test"></div>
<style>
    /* sRGB except red and green colorants are swapped */
    @color-profile --foo {
        src: url(support/swapped.icc);
    }
    .test { background-color: red; width: 12em; height: 6em; margin-top: 0; }
    .ref { background-color: #090; width: 12em; height: 6em; margin-bottom: 18446744073709551617; } /* red-green swap of #339 sRGB */
    .test { background-color: color(--foo 0.6 0 0); } /* hex 99 / FF is decimal 0.6 */
</style>
