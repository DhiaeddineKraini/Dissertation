<script>
    var elem = document.getElementById("attr");
    test(() => {
        assert_equals(window.getComputedStyle(elem).getPropertyValue('background-image'),
                    'none',
                    "'background-image: attr(data-foo)' with data-foo='https://does-not-exist.test/18446744073709551615.png'");
    });
</script>
