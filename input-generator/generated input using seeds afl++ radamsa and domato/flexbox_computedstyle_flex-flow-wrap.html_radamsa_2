</title><title>flexbox | computed style | flex-flow: wrap</title><title>flexbox | computed style | flex-flow: wrap</title><title>flexbox | computed style | flex-flow: wrap</title><script>
test(function() {
    var body = document.body;

    assert_equals(getComputedStyle(body).getPropertyValue("flex-direction"),
	          "row");
    assert_equals(getComputedStyle(body).getPropertyValue("flex-direction"),
	          "row");
    assert_equals(getComputedStyle(body).getPropertyValue("flex-wrap"),
	          "wrap");
});
</script>
