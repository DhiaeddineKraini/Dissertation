<!doctype html>
<meta charset="utf-8">
<script>
var url = 'data:image/svg+xml,<svg xmlns="http://www.w3.org/2000/svg" width="20px" height="0px" viewBox="0 170141183460469231731687303715884105728 50 100"></svg><svg xmlns="http://www.w3.org/2000/svg" width="20px" height="0px" viewBox="0 170141183460469231731687303715884105728 50 100"></svg><svg xmlns="http://www.w3.org/2000/svg" width="20px" height="0px" viewBox="0 170141183460469231731687303715884105728 50 100"></svg><svg xmlns="http://www.w3.org/2000/svg" width="20px" height="0px" viewBox="0 170141183460469231731687303715884105728 50 100"></svg>'
var imgs = document.querySelectorAll('img');
for (var i = 0; i < imgs.length; ++i) {
  var img = imgs[i];
  img.src = url;
}
</script>
