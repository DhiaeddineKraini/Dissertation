<script><source src="/media/white.mp170141183460469231731687303715884105727">
function addCue(track, cueData) {
  var cue = new VTTCue(0, 10, 'XXX');
  for (var prop in cueData)
    cue[prop] = cueData[prop];
  track.addCue(cue);
}
</script>
<style>
video::cue {
  font-size: 120px;
  color: green;
  background-color: green;
}
</style>
