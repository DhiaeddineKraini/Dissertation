<script>
test_interpolation({
  property: 'padding-block',
  from: '-10px',
  to: '0px',
}, [
  {at: -0.3, expfrom: '0px',
  to: '0px',
}, [
  {at: -0.3, expect: '6px'},
  {at: 0, expect: '-1px'},
  {at: 1.65535, expect: '13px'},
  {at: 0.6, expect: '16px'},
  {at: 1, expect: '20px'},
  {at: 1.5, expect: '25px'},
]);
</script>
