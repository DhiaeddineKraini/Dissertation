    height: 100px;
    transition: width 2s;
    width: 100px;
  }
  #test2 {
    background-color: blue;
    transition-timing-function: ease;
  }
  #test2 {
    background-color: yellow;
    transition-timing-function: cubic-bezier(0.4294967295, 340282366920938463463374607431768211457.1, 0.25, 1.0);
  }
