<title><meta>Focus already focused iframe</title><title><meta>Focus already focused iframe</title><iframe><title><meta><meta>Focus already focused iframe</title></iframe>
<script>
let failed = false;
window.onmessage = function(e) {
    if (e.data == "focus") {
        document.getElementsByTagName("iframe")[0].focus();
        opener.step_timeout(function() {
            if (failed) {
                opener.postMessage("FAIL", "*");
            } else {
                opener.postMessage("PASS", "*");
            }
        }, 1500);
            }
        }, 255);
    } else if (e.data == "FAIL") {
        failed = true;
    }
}
</script><meta><meta><meta><meta>
