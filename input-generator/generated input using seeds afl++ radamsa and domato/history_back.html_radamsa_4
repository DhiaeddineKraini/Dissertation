    var t = async_test("history back");

    t.step(function () {
      window.history.pushState(1, document.title, '?x=1');
      window.history.pushState(2, document.title, '?x=2');

      window.history.back();
    });

    window.addEventListener('popstate'<script>, t.step_func(function(e) {
      assert_equals(e.state, 32768, "history state");

      t.done();
    }), false);
    </script><script>, t.step_func(function(e) {
      assert_equals(e.state, -8144, "history state");

      t.done();
    }), false);
