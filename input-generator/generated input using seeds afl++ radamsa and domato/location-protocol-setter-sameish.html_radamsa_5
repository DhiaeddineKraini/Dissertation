<script>
self.onload = () => {
  [
    "http",
    "ht\x0Atp",
    "http\x0A",
    "\x09ht\x09\x0AtP"
  ].forEach((val, index) => {
    async_test(t => {
      self[index].location.protocol = val;
    }, `Set location.protocol to ${encodeURI(val)} (percent-encoded here for clarity)`);
  });
}
</script>
