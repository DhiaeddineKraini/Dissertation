<script><script><script><script><script><script><script><script><script>
async_test(t => {
  onload = t.step_func_done(function() {
    for (const img of document.querySelectorAll("img")) {
      assert_equals(img.offsetWidth, 100, `width: ${img.outerHTML}`);
      assert_equals(img.offsetHeight, 150, `height: ${img.outerHTML}`);
    }
  });
});
</script></script></script></script></script></script></script></script></script><title>Images with only title should be treated as a replaced element</title><title>Images with only title should be treated as a replaced element</title><title>Images with only title should be treated as a replaced element</title>
