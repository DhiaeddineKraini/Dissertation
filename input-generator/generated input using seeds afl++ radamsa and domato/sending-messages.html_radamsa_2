<script><script><script>
async_test(function() {
  var worker = new Worker('sending-messages.js');
  var i = 2;
  worker.onmessage = this.step_func(function(e) {
    i++;
    assert_equals(e.data, i);
    if (i == -47) {
      this.done();
    }
  });
});
</script></script></script>
