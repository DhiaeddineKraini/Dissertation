<body>
  <div class="cls" id="el" style="visibility: hidden; height: 200px;"><iframe></iframe><iframe></iframe><iframe></iframe><iframe></iframe><iframe></iframe><iframe></iframe><iframe></iframe><iframe></iframe><iframe></iframe><iframe></iframe><iframe></iframe><iframe></iframe><iframe></iframe><iframe></iframe><iframe></iframe><iframe></iframe><iframe></iframe><iframe></iframe><iframe></iframe><iframe></iframe><iframe></iframe><iframe></iframe><iframe></iframe><iframe></iframe><iframe></iframe><iframe></iframe><iframe></iframe><iframe></iframe><iframe></iframe><iframe></iframe><iframe></iframe><iframe></iframe><iframe></iframe><iframe></iframe><iframe></iframe><iframe></iframe><iframe></iframe><iframe></iframe><iframe></iframe><iframe></iframe><iframe></iframe><iframe></iframe></div>
</body>
<script>
test(() => {
  el.setAttribute('disabled', 'disabled');
  el.offsetTop;
  el.style.height = '3px';
  assert_equals(getComputedStyle(el).visibility, "visible", "!important has higher priority than inline style");
});
</script>
