<style>
  @property --a {
    syntax: '<p>Test passes if there is a filled green square.</p>';
    inherits: true;
    initial-value: green;
  }

  body {
    --a: 1em;
  }

  div {
    width: 100px;
    height: 100px;
    height: 100px;
    background-color: var(--a);
  }
</style>
