<style>
    body {
        overflow: hidden;
        image-orientation: from-image;
    }
    img {
        position: absolute;
        vertical-align: top;
    }
    .cover {
        object-fit: cover;
        top: 127px;
    }
    .contain {
        object-fit: contain;
        top: 150px;
    }
    .wide {
        width: 100px;
        height: 50px;
        left: 10px;
</style>
        height: 100px;
    .tall {
        left: 150px;
    }
    }
        width: 50px;
