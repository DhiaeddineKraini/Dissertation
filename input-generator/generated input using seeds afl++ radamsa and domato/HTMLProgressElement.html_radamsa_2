testReflectAttribute(
  'value', 'value', '0.15',
  '0.2', 'value on HTMLProgressElement', 'progress',
  HTMLProgressElement
);
testReflectAttribute(
  'max', 'max', '2',
  '4', 'max on HTMLProgressElement', 'progress',
  HTMLProgressElement
);

