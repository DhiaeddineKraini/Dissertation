    <style type="text/css">
    body { counter-reset: foo 1 Foo 5; }
    div:before { content: counters(foo, ".") "-" counters(Foo, "."); }
    div.a { counter-increment: foo; }
    div.b { counter-increment: Foo 2; }
    </style>
