<script id="code" type="text/worklet">
var testsPassed = false;
try {
    var c = function() { };
    c.prototype.paint = function() { };
    registerPaint('foo10', c);
    testsPassed = true;
} catch(ex) {
    testsPassed = false;
}

registerPaint('geometry', class {
    paint(ctx, geom) {
        if (testsPassed)
            ctx.fillStyle = 'green';
        else
            ctx.fillStyle = 'red';
        ctx.fillRect(0, 0, geom.width, geom.height);
    }
});
</script>

