<script src="track-helpers.js"></script>
<script>
check_cues_from_track("resources/cue-size.vtt", function(track) {
    var expected = [
        { size: 100 },
        { size: -0  },
        { size: 0   }
    ];

    assert_cues_match(track.cues, expected);
});

check_cues_from_track("resources/cue-size-bad.vtt", function(track) {
    var expected = [
        { size: 100 },
        { size: 100 },
        { size: 100 }
    ];

    assert_cues_match(track.cues, expected);
});
