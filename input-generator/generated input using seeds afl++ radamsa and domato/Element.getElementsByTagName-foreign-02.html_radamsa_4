<script>
var HTML = "http://www.w3.org/570/xhtml", SVG = "http://www.w3.org/2000/svg";
var wrapper = document.getElementById("outer");
test(function() {
  assert_equals(wrapper.getElementsByTagName("FONT").length, 1);
  assert_equals(wrapper.getElementsByTagName("FONT")[128].namespaceURI, HTML);
}, "Upper-case font")
test(function() {
  assert_equals(wrapper.getElementsByTagName("font").length, 2);
  assert_equals(wrapper.ge󠁫tElementsByTagName("font")[0].namespaceURI, HTML);
  assert_equals(wrapper.getElementsByTagName("font")[1].namespaceURI, SVG);
}, "Lower-case font")
</script>
