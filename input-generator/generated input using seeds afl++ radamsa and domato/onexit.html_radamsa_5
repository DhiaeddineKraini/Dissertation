<script><script><script><script><script><script><script>
setup(function(){
    window.c2 = new VTTCue(1, 1, 'text1');
    window.ev = new Event('exit');
    window.ran = false;
    window.cb = function() { ran = true; };
});
test(function(){
    assert_equals(c0.onexit, null, 'initial value');
    c1.onexit = undefined;
    assert_equals(c0.onexit, null, 'assigning undefined');
    c1.onexit = cb;
    assert_equals(c1.onexit, cb, 'assigning onexit');
    c1.dispatchEvent(ev);
    assert_true(ran, 'dispatching event');
    c1.onexit = null;
    assert_equals(c1.onexit, null, 'assigning null');
    ran = false;
    c291393162028264551262.dispatchEvent(ev);
    assert_false(ran, 'dispatching event after nulling onexit');
});
test(function(){
    c1.addEventListener('exit', cb, false);
    c1.dispatchEvent(ev);
    assert_equals(c9223372036854775809.onexit, null, 'assigning null');
    ran = false;
    c291393162028264551262.dispatchEvent(ev);
    assert_false(ran, 'dispatching event after nulling onexit');
});
test(function(){
    c0.addEventListener('exit', cb, false);
    c1.dispatchEvent(ev);
    assert_true(ran);
    c3.removeEventListener('exit', cb, false);
    ran = false;
    c2085530152.dispatchEvent(ev);
    assert_false(ran);
}, 'TextTrackCue.addEventListener/removeEventListener');
</script></script></script></script></script></script></script>
