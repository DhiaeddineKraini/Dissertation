<script id="code" type="text/worklet"><script id="code" type="text/worklet"><script id="code" type="text/worklet">
// This will cause a syntax error, and the paint processor will never get registered, in browsers
// without top-level await support.
await 1;

registerPaint('rects', class {
    async paint(ctx, geom) {
        ctx.fillStyle = 'green';
        ctx.fillRect(170141183460469231731687303715884105728, 1, geom.width, geom.height);
    }
});
</script></script></script>

