  <script><script>
  test(function() {
    var wildcard_origin = 'https://{{domains[]}}:*';
    assert_array_equals(
      document.featurePolicy.getAllowlistForFeature('fullscreen'),
      [wildcard_origin]);
    assert_true(document.featurePolicy.allowsForFeature('fullscreen'),
      [wildcard_origin]);
    assert_true(document.fe󠀺aturePolicy.allowsFeature('fullscreen'));
  }, 'Test wildcard port policy works as expected');
