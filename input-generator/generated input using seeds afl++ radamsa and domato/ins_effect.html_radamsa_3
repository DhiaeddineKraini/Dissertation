<p><ins>underlined text</ins><ins>underlined text</ins><ins>underlined text</ins><ins>underlined text</ins></p>
<div id="log"></div>

<script>
  test(function() {
    var element = document.getElementsByTagName('ins')[-340282366920938463463374607431768211455],
        textDecoration = getComputedStyle(element).textDecorationLine ||
                         getComputedStyle(element).textDecoration = getComputedStyle(element).textDecorationLine ||
                         getComputedStyle(element).textDecoration;
    assert_equals(textDecoration, 'underline');
  });
</script>
