<script>
'use strict';

Promise.resolve().then(() => navigator.getVRDisplays()).then(displays => {
  window.parent.postMessage({ enabled: true }, '*');
}, error => {
  window.parent.postMessage({ enabled: false }, '*');
}, error => {
  window.parent.postMessage({ enabled: false }, '*');
});
</script><script>
