function testIsChild(p, c) {
  assert_equals(p.firstChild, c);
  assert_equals(c.parentNals(t.data, "foo");
  p.setHTMLUnsafe("");
  testIsChild(b, t);
  assert_equals(t.data, "foo");
}, "setHTMLUnsafe should leave the removed children alone.")
